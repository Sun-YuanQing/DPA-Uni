<view hidden="{{!(showView)}}" class="unpack-view"><view class="uni-mask" style="{{'top:'+(offsetTop+'px')+';'}}"></view><view class="uni-modal"><view class="uni-modal__hd"><view class="uni-modal__title _strong">{{title}}</view></view><view class="uni-modal__bd"><view class="uni-list-cell"><view class="uni-list-cell-left">料号:</view><view class="uni-list-cell-db">{{''+model.partItemNo+''}}</view></view><view class="uni-list-cell input-row"><view class="uni-list-cell-left">数量:</view><view class="uni-list-cell-db">{{''+model.sheetQty+''}}</view></view><view class="uni-list-cell input-row"><view class="uni-list-cell-left">批次:</view><view class="uni-list-cell-db">{{''+model.inLot+''}}</view></view><view class="uni-list-cell input-row"><view class="uni-list-cell-left">数量:</view><view class="uni-list-cell-db"><input type="number" data-event-opts="{{[['input',[['__set_model',['','numQty','$event',[]]]]]]}}" value="{{numQty}}" bindinput="__e"/></view><view class="uni-list-cell-left">包数:</view><view class="uni-list-cell-db"><input type="number" data-event-opts="{{[['input',[['__set_model',['','num','$event',[]]]]]]}}" value="{{num}}" bindinput="__e"/></view><view class="uni-list-cell-left"><button style="padding:0;height:25px;width:25px;line-height:1.3;" type="default" data-event-opts="{{[['tap',[['onNumAdd',['$event']]]]]}}" bindtap="__e"><label style="text-align:center;" class="_span">+</label></button></view></view><view class="uni-list-cell" style="height:30px;"><view class="flex-item"><view class="_strong">数量</view></view><view class="flex-item"><view class="_strong">包数</view></view></view><scroll-view style="height:350rpx;" scroll-y="true"><view class="uni-list"><block wx:for="{{details}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><uni-swipe-action vue-id="{{'1-'+index}}" options="{{options1}}" det_id="{{item.det_id}}" data-event-opts="{{[['^click',[['bindClick']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><uni-list-item vue-id="{{('2-'+index)+','+('1-'+index)}}" show-arrow="{{false}}" bind:__l="__l" vue-slots="{{['default']}}"><view class="uni-flex uni-row"><view class="flex-item">{{item.numQty}}</view><view class="flex-item">{{item.num}}</view></view></uni-list-item></uni-swipe-action></block></block></view></scroll-view><block wx:if="{{isShowSaveSelected}}"><view style="border-top:1rpx solid #ccc;margin-left:20rpx;padding-top:10rpx;"><label data-event-opts="{{[['tap',[['cbxChange',['$event']]]]]}}" class="checkbox" bindtap="__e"><checkbox checked="{{isSaveSelected}}"></checkbox>下次不再选择</label></view></block></view><view class="uni-modal__ft"><view data-event-opts="{{[['tap',[['hide',['$event']]]]]}}" class="uni-modal__btn uni-modal__btn_default" style="color:rgb(0, 0, 0);" bindtap="__e">取消</view><view data-event-opts="{{[['tap',[['onSubmitPrint',['$event']]]]]}}" class="uni-modal__btn uni-modal__btn_primary" style="color:rgb(0, 122, 255);" bindtap="__e">打印</view></view></view><label-print class="vue-ref" vue-id="3" progNo="{{progNo}}" showReport="{{showReport}}" reportModel="{{reportModel}}" data-ref="labPri" data-event-opts="{{[['^printCancel',[['printCancel']]],['^printComplete',[['printComplete']]]]}}" bind:printCancel="__e" bind:printComplete="__e" bind:__l="__l"></label-print></view>