<view><view id="viewHeader"><barcode-input class="vue-ref" vue-id="1" focus="{{true}}" placeholder="客户订单号" data-ref="input_order" data-event-opts="{{[['^onScaned',[['onScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input></view><scroll-view style="{{'height:'+(scrollHeight)+';'}}" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view class="uni-list"><block wx:for="{{details}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['uni-list-cell scroll-view-item',item.selectItemClass]}}" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['onSelectedItem',[index]]]]]}}" bindtap="__e"><view class="uni-triplex-row"><view class="uni-triplex-left"><text class="uni-title">{{"发货单号:"+item.sheetNo}}</text><text class="uni-title" style="min-width:500rpx;">{{"客户订单号:"+item.cuOrdNo}}</text><text class="uni-title" style="min-width:500rpx;">{{"料品编码:"+item.partItemNo}}</text><text class="uni-title" style="min-width:500rpx;">{{"料品名称:"+item.partName}}</text></view><view class="uni-triplex-right"><text class="uni-h5" style="min-width:200rpx;margin-left:-500rpx;">{{"数量:"+item.sheetQty}}</text><text class="uni-h5" style="min-width:200rpx;margin-left:-500rpx;">{{"单位名称:"+item.unitName}}</text></view></view></view></block></block></view></scroll-view></view>