<view><view id="viewHeader"><barcode-input class="vue-ref" vue-id="1" focus="{{true}}" clearScan="{{false}}" placeholder="请扫描标签" data-ref="input_order" data-event-opts="{{[['^onScaned',[['onScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input><barcode-input class="vue-ref" vue-id="2" placeholder="请扫描新标签" data-ref="input_part" data-event-opts="{{[['^onScaned',[['onPartScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input></view><scroll-view style="{{'height:'+(scrollHeight)+';'}}" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view class="uni-list"><block wx:for="{{details}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['uni-list-cell scroll-view-item',item.selectItemClass]}}" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['onSelectedItem',[index]]]]]}}" bindtap="__e"><view class="uni-triplex-row"><view class="uni-triplex-left"><text class="uni-title">{{"料号:"+item.partItemNo}}</text><text class="uni-title">{{"料品名称:"+item.partName}}</text><text class="uni-title">{{isStorageBin?'货架:'+item.binNo:'库位:'+item.binWhNo}}</text><text class="uni-title">{{"数量: "+item.sheetQty}}</text></view></view></view></block></block></view></scroll-view><page-foot class="vue-ref" vue-id="3" data-ref="viewBtm" bind:__l="__l" vue-slots="{{['default']}}"><button style="width:160px;margin-right:10px;float:left;" type="default" data-event-opts="{{[['tap',[['onShowPrint',['$event']]]]]}}" bindtap="__e">分拆</button><button style="display:block;float:none;" type="primary" data-event-opts="{{[['tap',[['onSave',['$event']]]]]}}" bindtap="__e">保存</button></page-foot><lotunpack vue-id="4" show="{{showLotUnpack}}" model="{{printMode}}" progNo="{{progNo}}" isSaveSplit="{{false}}" data-event-opts="{{[['^cancel',[['lotupackCancel']]]]}}" bind:cancel="__e" bind:__l="__l"></lotunpack></view>