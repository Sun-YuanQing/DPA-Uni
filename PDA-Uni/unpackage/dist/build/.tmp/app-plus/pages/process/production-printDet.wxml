<view><view id="viewHeader"><barcode-input class="vue-ref" vue-id="1" focus="{{true}}" placeholder="扫描内标签" data-ref="input_part" data-event-opts="{{[['^onScaned',[['onPartScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input><view class="uni-list-cell input-row"><view class="uni-list-cell-left">来源批号:</view><view class="uni-list-cell-db">{{order.poMoSoLot}}</view></view><view class="uni-list-cell input-row"><view class="uni-list-cell-left">物料编码:</view><view class="uni-list-cell-db">{{order.partItemNo}}</view></view><view class="uni-list-cell input-row"><view class="uni-list-cell-left">客户编码:</view><view class="uni-list-cell-db">{{order.citemNo}}</view></view><view class="uni-list-cell input-row"><view class="uni-list-cell-left">客户料号:</view><view class="uni-list-cell-db">{{order.suPartNo}}</view></view><view class="uni-list-cell input-row"><view class="uni-list-cell-left">工单数量:</view><view class="uni-list-cell-db">{{order.sheetQty}}</view></view><view class="uni-list-cell input-row"><view class="uni-list-cell-left">已产生数量:</view><view class="uni-list-cell-db">{{order.createTagQty}}</view><view class="uni-list-cell-left">未产生数量:</view><view class="uni-list-cell-db">{{order.lastTagQty}}</view></view></view><view style="position:relative;"><block wx:if="{{checked_all_show}}"><view data-event-opts="{{[['tap',[['onAll_100',['$event']]]]]}}" style="position:absolute;top:20rpx;left:20rpx;z-index:98;border:#007AFF 1px solid;border-radius:10rpx;background:#D5E3F0;" bindtap="__e"><view style="float:left;">全选：</view><view style="float:left;width:50rpx;height:50rpx;border-radius:50%;border:#007AFF 1px solid;z-index:99;overflow:hidden;"><checkbox style="transform:scale(1.3);" checked="{{checked_all}}"></checkbox></view></view></block></view><scroll-view style="{{'height:'+(scrollHeight)+';'}}" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view class="uni-list"><block wx:for="{{details}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['uni-list-cell scroll-view-item',item.selectItemClass]}}" hover-class="uni-list-cell-hover"><view data-event-opts="{{[['tap',[['onSelectedItem',[index]]]]]}}" class="uni-triplex-row" bindtap="__e"><view class="uni-triplex-left"><text class="uni-title">{{"标签ID:"+item.tagId}}</text><checkbox checked="{{item.checked}}"></checkbox></view><view class="uni-triplex-right"><text class="uni-h5" style="margin-left:-220rpx;">{{"单位:"+item.partSpec}}</text><text class="uni-h5" style="margin-left:-220rpx;">{{"数量:"+item.sheetQty}}</text></view></view></view></block></block></view></scroll-view><page-foot class="vue-ref" vue-id="2" data-ref="viewBtm" bind:__l="__l" vue-slots="{{['default']}}"><print-button class="vue-ref" style="padding:0rpx;width:48%;margin-right:10px;float:left;" vue-id="{{('3')+','+('2')}}" reportModel="{{reportModel}}" data-ref="printBtn" data-event-opts="{{[['^click',[['onPrint']]],['^printComplete',[['printComplete']]],['^tagValid',[['tagValid']]],['^printCancel',[['onprintCancel']]]]}}" bind:click="__e" bind:printComplete="__e" bind:tagValid="__e" bind:printCancel="__e" bind:__l="__l"></print-button><button style="padding:0rpx;float:none;" type="primary" data-event-opts="{{[['tap',[['onDelete',['$event']]]]]}}" bindtap="__e">删除</button></page-foot></view>