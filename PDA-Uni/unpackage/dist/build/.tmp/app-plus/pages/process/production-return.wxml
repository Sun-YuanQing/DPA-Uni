<view><view id="viewHeader"><barcode-input class="vue-ref" vue-id="1" focus="true" placeholder="请扫描制造单批号" data-ref="input_order" data-event-opts="{{[['^onScaned',[['onScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input></view><scroll-view style="{{'height:'+(scrollHeight)+';'}}" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view class="uni-list"><block wx:for="{{details}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['uni-list-cell scroll-view-item',item.selectItemClass]}}" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['onSelectedItem',[index]]]]]}}" bindtap="__e"><view class="uni-triplex-row"><view class="uni-triplex-left"><text class="uni-title uni-ellipsis">{{"退库单号:"+item.sheetNo}}</text><text class="uni-title uni-ellipsis">{{"制造单批号:"+item.purLot}}</text><text class="uni-title">{{"料号:"+item.partItemNo}}</text><text class="uni-title">{{"料品名称:"+item.partName}}</text></view><view class="uni-triplex-right" style="min-width:220rpx;"><text class="uni-h5">{{"退置仓库:"+item.whNo}}</text><text class="uni-h5">{{"单位:"+item.unitName}}</text><text class="uni-h5">{{"数量: "+item.sheetQty}}</text></view></view></view></block></block></view></scroll-view></view>