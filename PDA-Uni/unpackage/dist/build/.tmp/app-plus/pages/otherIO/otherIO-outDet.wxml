<view><view id="viewHeader"><barcode-input class="vue-ref" vue-id="1" focus="{{true}}" placeholder="请扫描料号" data-ref="input_part" data-event-opts="{{[['^onScaned',[['onPartScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input></view><scroll-view style="{{'height:'+(scrollHeight)+';'}}" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view class="uni-list"><block wx:for="{{order.details}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['uni-list-cell scroll-view-item',item.selectItemClass]}}" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['onSelectedItem',[index]]]]]}}" bindtap="__e"><view class="uni-triplex-row"><view class="uni-triplex-left"><text class="uni-title">{{"单号:"+item.sheetNo}}</text><text class="uni-title">{{"单据时间:"+item.sheetDate}}</text><text class="uni-title">{{"料号:"+item.partItemNo}}</text><text class="uni-title">{{"料品名称:"+item.partName}}</text><text class="uni-title">{{"仓库:"+(item.whName?item.whName:'')+(item.whNo?'('+item.whNo+')':'')}}</text><block wx:if="{{isStorageBin}}"><text class="uni-title">{{"货架:"+(item.binName?item.binName:'')+(item.binNo?'('+item.binNo+')':'')}}</text></block></view><view class="uni-triplex-right"><text class="uni-h5" style="min-width:200rpx;margin-left:-300rpx;">{{"批号:"+item.sheetLot}}</text><text class="uni-h5" style="min-width:200rpx;margin-left:-300rpx;">{{"单位:"+item.unitName}}</text><text class="uni-h5" style="min-width:200rpx;margin-left:-300rpx;">{{"数量:"+item.sheetQty}}</text><text class="uni-h5" style="min-width:200rpx;margin-left:-300rpx;">{{"条码:"+(item.m_isScan?'已较对':'未较对')}}</text></view></view></view></block></block></view></scroll-view><page-foot class="vue-ref" vue-id="2" data-ref="viewBtm" bind:__l="__l" vue-slots="{{['default']}}"><button type="primary" data-event-opts="{{[['tap',[['onSave',['$event']]]]]}}" bindtap="__e">保存</button></page-foot><lotunpack vue-id="3" show="{{showLotUnpack}}" model="{{printMode}}" printDetail="{{printDetail}}" progNo="{{progNo}}" data-event-opts="{{[['^cancel',[['lotupackCancel']]]]}}" bind:cancel="__e" bind:__l="__l"></lotunpack></view>