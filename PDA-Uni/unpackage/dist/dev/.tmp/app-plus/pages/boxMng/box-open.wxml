<view><view id="viewHeader"><barcode-input class="vue-ref" vue-id="1" focus="{{true}}" placeholder="请扫描外箱包装" data-ref="input_box" data-event-opts="{{[['^onScaned',[['onBoxScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input></view><scroll-view style="{{'height:'+(scrollHeight)+';'}}" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view class="uni-list"><block wx:for="{{details}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['uni-list-cell scroll-view-item',item.selectItemClass]}}" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['onSelectedItem',[index]]]]]}}" bindtap="__e"><view class="uni-triplex-row"><view class="uni-triplex-left"><text class="uni-title">{{"标签ID:"+item.tagId}}</text><text class="uni-title">{{"料品编码:"+item.partItemNo}}</text><text class="uni-title">{{"料品名称:"+item.partName}}</text></view><view class="uni-triplex-right" style="min-width:220rpx;"><text class="uni-h5">{{"单位:"+item.unitName}}</text><text class="uni-h5">{{"数量:"+item.sheetQty}}</text></view></view></view></block></block></view></scroll-view><page-foot class="vue-ref" vue-id="2" data-ref="viewBtm" bind:__l="__l" vue-slots="{{['default']}}"><button type="primary" data-event-opts="{{[['tap',[['onSave',['$event']]]]]}}" bindtap="__e">拆箱</button></page-foot></view>