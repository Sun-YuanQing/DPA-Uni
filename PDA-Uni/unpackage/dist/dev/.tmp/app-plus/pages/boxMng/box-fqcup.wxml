<view><view id="viewHeader"><barcode-input class="vue-ref" vue-id="1" focus="{{true}}" clearScan="{{false}}" placeholder="请扫描工单批号" data-ref="input_order" value="{{purLot}}" data-event-opts="{{[['^onScaned',[['onScaned']]],['^input',[['__set_model',['','purLot','$event',[]]]]]]}}" bind:onScaned="__e" bind:input="__e" bind:__l="__l"></barcode-input><view class="uni-list-cell input-row"><view class="uni-list-cell-left">抽检数量:</view><view class="uni-list-cell-db"><input class="vue-ref" type="number" placeholder="0" focus="{{testNumFocus}}" data-ref="input_testNum" data-event-opts="{{[['blur',[['onTestNumBlur',['$event']]]],['input',[['__set_model',['','testNum','$event',[]]]]]]}}" value="{{testNum}}" bindblur="__e" bindinput="__e"/></view></view><view class="uni-list-cell input-row"><view class="uni-list-cell-left">送检数量:</view><view class="uni-list-cell-db"><input class="vue-ref" type="number" placeholder="0" focus="{{sheetQtyFocus}}" data-ref="input_sheetQty" data-event-opts="{{[['blur',[['onsheetQtyBlur',['$event']]]],['input',[['__set_model',['','sheetQty','$event',[]]]]]]}}" value="{{sheetQty}}" bindblur="__e" bindinput="__e"/></view></view><barcode-input class="vue-ref" vue-id="2" placeholder="请扫描外箱包装" data-ref="input_box" data-event-opts="{{[['^onScaned',[['onBoxScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input><barcode-input class="vue-ref" vue-id="3" placeholder="请扫描料号" data-ref="input_part" data-event-opts="{{[['^onScaned',[['onPartScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input><view><view class="uni-list-cell input-row"><view class="uni-list-cell-left">料品编码:</view><view class="uni-list-cell-db">{{order.partItemNo}}</view></view><view class="uni-list-cell input-row"><view class="uni-list-cell-left">未送检数量:</view><view class="uni-list-cell-db">{{order.receiveQty}}</view><view class="uni-list-cell-left">送检数量:</view><view class="uni-list-cell-db">{{sheetQty}}</view></view></view></view><scroll-view style="{{'height:'+(scrollHeight)+';'}}" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view class="uni-list"><block wx:for="{{boxlist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['uni-list-cell scroll-view-item',item.selectItemClass]}}" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['onSelectedItem',[index]]]]]}}" bindtap="__e"><view class="uni-triplex-row"><view class="uni-triplex-left"><text class="uni-title">{{"箱标签ID:"+item.tagId}}</text><text class="uni-title">{{"料品编码:"+item.partItemNo}}</text><text class="uni-title">{{"料品名称:"+item.partName}}</text><text class="uni-title">{{"数量:"+item.sheetQty}}</text></view><view class="uni-triplex-right"><text class="uni-h5" style="margin-left:-200px;">{{"单位:"+item.unitName}}</text><text class="uni-h5" style="margin-left:-200px;">{{"已检数量:"+item.scanQty}}</text><text class="uni-h5" style="margin-left:-200px;">{{"未检数量:"+item.lastScanQty}}</text></view></view></view></block></block></view></scroll-view><page-foot class="vue-ref" vue-id="4" data-ref="viewBtm" bind:__l="__l" vue-slots="{{['default']}}"><button style="width:49%;margin-right:10px;float:left;" type="primary" data-event-opts="{{[['tap',[['onSave',[true]]]]]}}" bindtap="__e">QC合格</button><button data-event-opts="{{[['tap',[['onSave',[false]]]]]}}" bindtap="__e">QC不合格</button></page-foot></view>