<view><view id="viewHeader"><barcode-input class="vue-ref" vue-id="1" focus="true" placeholder="请扫描下架料号" data-ref="input_part" data-event-opts="{{[['^onScaned',[['onPartScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input><barcode-input class="vue-ref" vue-id="2" placeholder="{{placeholder_store}}" clearScan="{{false}}" data-ref="input_store" data-event-opts="{{[['^onScaned',[['onScanStorage']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input><barcode-input class="vue-ref" vue-id="3" placeholder="请扫描上架料号" data-ref="input_part2" data-event-opts="{{[['^onScaned',[['onPartScaned2']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input></view><scroll-view style="{{'height:'+(scrollHeight)+';'}}" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view class="uni-list"><block wx:for="{{details}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['uni-list-cell scroll-view-item',item.selectItemClass]}}" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['onSelectedItem',[index]]]]]}}" bindtap="__e"><view class="uni-triplex-row"><view class="uni-triplex-left"><text class="uni-title">{{"料号:"+item.model.partItemNo}}</text><text class="uni-title">{{"批次:"+item.model.inLot}}</text><text class="uni-title">{{"数量: "+item.model.sheetQty}}</text><text class="uni-title">{{"下架库位: "+item.model.reservelocus}}</text><text class="uni-title">{{"上架库位:"+(isStorageBin?item.model.binNo_Up?item.model.binNo_Up:'':item.model.binWhNo_Up?item.model.binWhNo_Up:'')}}</text></view><view class="uni-triplex-right" style="min-width:220rpx;"><text class="uni-h5">{{"状态:"+item.model.stateName}}</text></view></view></view></block></block></view></scroll-view><page-foot class="vue-ref" vue-id="4" data-ref="viewBtm" bind:__l="__l" vue-slots="{{['default']}}"><button type="primary" data-event-opts="{{[['tap',[['save',['$event']]]]]}}" bindtap="__e">保存</button></page-foot></view>