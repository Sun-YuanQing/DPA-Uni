<view><view id="viewHeader"><barcode-input class="vue-ref" vue-id="1" focus="true" clearScan="{{true}}" placeholder="{{placeholder_store}}" data-ref="input_store" data-event-opts="{{[['^onScaned',[['onScanStorage']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input><barcode-input class="vue-ref" vue-id="2" placeholder="请扫描料号" data-ref="input_part" data-event-opts="{{[['^onScaned',[['onPartScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input><view><view class="uni-list-cell input-row"><view class="uni-list-cell-left">存放位置:</view><view class="uni-list-cell-db">{{''+(binNo||binWhNo)+''}}</view></view><view class="uni-list-cell input-row"><view class="uni-list-cell-left">扫描数量:</view><view class="uni-list-cell-db">{{''+totalQty+''}}</view><view class="uni-list-cell-left">扫描个数:</view><view class="uni-list-cell-db">{{''+details.length+''}}</view></view></view></view><scroll-view style="{{'height:'+(scrollHeight)+';'}}" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view class="uni-list"><block wx:for="{{details}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['uni-list-cell scroll-view-item',item.selectItemClass]}}" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['onSelectedItem',[index]]]]]}}" bindtap="__e"><view class="uni-triplex-row"><view class="uni-triplex-left"><text class="uni-title">{{"料号:"+item.model.partItemNo}}</text><text class="uni-title">{{"批次:"+item.model.inLot}}</text><text class="uni-title">{{(isStorageBin?'货架:'+binNo:'上架库位:'+binWhNo)+''}}</text><text class="uni-title">{{"数量: "+item.model.sheetQty}}</text><text class="uni-title">{{"标签: "+item.model.tagId}}</text></view></view></view></block></block></view></scroll-view><page-foot class="vue-ref" vue-id="3" data-ref="viewBtm" bind:__l="__l" vue-slots="{{['default']}}"><button type="primary" data-event-opts="{{[['tap',[['onSave',['$event']]]]]}}" bindtap="__e">保存</button