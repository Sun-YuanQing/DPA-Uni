<view><view id="viewHeader"><barcode-input class="vue-ref" vue-id="1" focus="{{true}}" clearScan="{{false}}" placeholder="扫出货通知单" data-ref="input_order" data-event-opts="{{[['^onScaned',[['onScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input><barcode-input class="vue-ref" vue-id="2" placeholder="扫包装料号" data-ref="input_part" data-event-opts="{{[['^onScaned',[['onPartScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input></view><scroll-view style="{{'height:'+(scrollHeight)+';'}}" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view class="uni-list"><block wx:for="{{details}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['uni-list-cell scroll-view-item',item.selectItemClass]}}" hover-class="uni-list-cell-hover"><view class="uni-flex uni-column" style="width:100%;"><view class="uni-triplex-row"><view class="uni-triplex-left"><text class="uni-title">{{"检验单号:"+item.sheetNo}}</text><text class="uni-title">{{"料号:"+item.partItemNo}}</text><text class="uni-title">{{"名称:"+item.partName}}</text><text class="uni-title">{{"检验数量: "+item.sheetQty}}</text></view><view class="uni-triplex-right" style="min-width:220rpx;"><text class="uni-h5" style="margin-left:-300px;">{{"已扫数量:"+item.scanQty}}</text><text class="uni-h5" style="margin-left:-300px;">{{"未扫数量:"+item.lastScanQty}}</text></view></view></view></view></block></block></view></scroll-view><page-foot class="vue-ref" vue-id="3" data-ref="viewBtm" bind:__l="__l" vue-slots="{{['default']}}"><button style="display:block;float:none;" type="primary" data-event-opts="{{[['tap',[['onSave',['$event']]]]]}}" bindtap="__e">保存</button></page-foot><authorize-input vue-id="4" show="{{showAuth}}" lockModel="{{lockModel}}" data-event-opts="{{[['^onClick',[['authClick']]]]}}" bind:onClick="__e" bind:__l="__l"></authorize-input></view>