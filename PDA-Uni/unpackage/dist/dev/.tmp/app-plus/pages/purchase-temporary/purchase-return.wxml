<view><view id="viewHeader"><barcode-input class="vue-ref" vue-id="1" focus="true" placeholder="{{placeholder_order}}" data-ref="input_order" data-event-opts="{{[['^onScaned',[['onScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input><barcode-input class="vue-ref" vue-id="2" placeholder="请扫描料号" data-ref="input_part" data-event-opts="{{[['^onScaned',[['onPartScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input><view><view class="uni-list-cell input-row"><view class="uni-list-cell-left">单号：</view><view class="uni-list-cell-db">{{order.sheetNo}}</view></view><view class="uni-list-cell input-row"><view class="uni-list-cell-left">单据日期：</view><view class="uni-list-cell-db">{{order.sheetDate}}</view></view><view class="uni-list-cell input-row"><view class="uni-list-cell-left">供应商：</view><view class="uni-list-cell-db">{{order.custName1}}</view></view></view></view><scroll-view style="{{'height:'+(scrollHeight)+';'}}" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view class="uni-list"><block wx:for="{{order.details}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['uni-list-cell scroll-view-item',item.selectItemClass]}}" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['onSelectedItem',[index]]]]]}}" bindtap="__e"><view class="uni-triplex-row"><view class="uni-triplex-left"><text class="uni-title uni-ellipsis">{{"批次号:"+item.inLot}}</text><text class="uni-title">{{"料号:"+item.partItemNo}}</text><text class="uni-title">{{"料品名称:"+item.partName}}</text><text class="uni-title">{{"仓库:"+item.whName}}</text><text class="uni-title">{{"数量: "+item.sheetQty}}</text></view><view class="uni-triplex-right" style="min-width:220rpx;"><text class="uni-h5">{{"单位:"+item.unitName}}</text><text class="uni-h5">{{"已扫描数量:"+item.scanQty}}</text><text class="uni-h5">{{"未扫描数量:"+item.lastScanQty}}</text></view></view></view></block></block></view></scroll-view><page-foot class="vue-ref" vue-id="3" data-ref="viewBtm" bind:__l="__l" vue-slots="{{['default']}}"><button style="display:block;float:none;" type="primary" data-event-opts="{{[['tap',[['onSave',['$event']]]]]}}" bindtap="__e">保存</button></page-foot><lotunpack vue-id="4" show="{{showLotUnpack}}" model="{{printMode}}" printDetail="{{printDetail}}" progNo="{{progNo}}" data-event-opts="{{[['^cancel',[['lotupackCancel']]],['^printComplete',[['printComplete']]],['^printCancel',[['printCancel']]]]}}" bind:cancel="__e" bind:printComplete="__e" bind:printCancel="__e" bind:__l="__l"></lotunpack></view>