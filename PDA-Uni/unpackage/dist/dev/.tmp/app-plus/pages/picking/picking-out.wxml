<view><view id="viewHeader"><barcode-input class="vue-ref" vue-id="1" focus="{{true}}" clearScan="{{false}}" placeholder="请扫描工单号" data-ref="input_order" data-event-opts="{{[['^onScaned',[['onScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input><barcode-input class="vue-ref" vue-id="2" placeholder="请扫描料号" data-ref="input_part" data-event-opts="{{[['^onScaned',[['onPartScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input></view><scroll-view style="{{'height:'+(scrollHeight)+';'}}" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view class="uni-list"><block wx:for="{{details}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['uni-list-cell scroll-view-item',item.selectItemClass]}}" hover-class="uni-list-cell-hover"><view class="uni-flex uni-column" style="width:100%;"><view class="uni-triplex-row"><view class="uni-triplex-left"><text class="uni-title">{{"料号:"+item.partItemNo}}</text><text class="uni-title">{{"应领数量: "+item.neetQty}}</text></view><view class="uni-triplex-right" style="min-width:220rpx;"><text class="uni-h5">{{"实领数量:"+item.sheetQty}}</text><text class="uni-h5">{{"待领数量:"+item.lastScanQty}}</text></view></view><block wx:if="{{item.binLots.length>0}}"><view class="uni-triplex-row"><view class="div-table fixed-thead"><view class="thead"><view class="th"><view class="td" style="width:20%;">库位</view><view class="td" style="width:60%;">批次</view><view class="td" style="width:20%;">数量</view></view></view><view class="tbody"><block wx:for="{{item.binLots}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><block wx:if="{{index==0}}"><view class="tr"><view class="td" style="width:20%;font-weight:bold;font-size:14px;">{{isStorageBin?item.binNo:item.whNo}}</view><view class="td" style="width:60%;font-weight:bold;font-size:14px;">{{item.inLot}}</view><view class="td" style="width:20%;font-weight:bold;font-size:14px;">{{item.sheetQty}}</view></view></block><block wx:else><view class="tr"><view class="td" style="width:20%;">{{isStorageBin?item.binNo:item.whNo}}</view><view class="td" style="width:60%;">{{item.inLot}}</view><view class="td" style="width:20%;">{{item.sheetQty}}</view></view></block></block></block></view></view></view></block></view></view></block></block></view></scroll-view><page-foot class="vue-ref" vue-id="3" data-ref="viewBtm" bind:__l="__l" vue-slots="{{['default']}}"><button style="display:block;float:none;" type="primary" data-event-opts="{{[['tap',[['onSave',['$event']]]]]}}" bindtap="__e">保存</button></page-foot><authorize-input vue-id="4" show="{{showAuth}}" lockModel="{{lockModel}}" data-event-opts="{{[['^onClick',[['authClick']]]]}}" bind:onClick="__e" bind:__l="__l"></authorize-input><lotunpack vue-id="5" show="{{showLotUnpack}}" model="{{printMode}}" printDetail="{{printDetail}}" progNo="{{progNo}}" data-event-opts="{{[['^cancel',[['lotupackCancel']]]]}}" bind:cancel="__e" bind:__l="__l"></lotunpack></view>