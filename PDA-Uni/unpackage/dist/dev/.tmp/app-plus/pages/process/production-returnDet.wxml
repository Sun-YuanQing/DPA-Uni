<view><view id="viewHeader"><block wx:if="{{isStorageBin}}"><barcode-input class="vue-ref" vue-id="1" focus="true" clearScan="{{false}}" placeholder="{{placeholder_store}}" data-ref="input_store" data-event-opts="{{[['^onScaned',[['onScanStorage']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input></block><barcode-input class="vue-ref" vue-id="2" focus="{{isStorageBin==false}}" placeholder="请扫描料号" data-ref="input_part" data-event-opts="{{[['^onScaned',[['onPartScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input></view><scroll-view style="{{'height:'+(scrollHeight)+';'}}" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view class="uni-list"><block wx:for="{{details}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['uni-list-cell scroll-view-item',item.selectItemClass]}}" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['onSelectedItem',[index]]]]]}}" bindtap="__e"><view class="uni-triplex-row"><view class="uni-triplex-left"><text class="uni-title">{{"料号:"+item.partItemNo}}</text><text class="uni-title">{{"料品名称:"+item.partName}}</text><text class="uni-title">{{"数量: "+item.sheetQty}}</text></view><view class="uni-triplex-right"><text class="uni-h5" style="min-width:200rpx;margin-left:-300rpx;">{{"单位:"+item.unitName}}</text><text class="uni-h5" style="min-width:200rpx;margin-left:-300rpx;">{{"已扫描数量:"+item.scanQty}}</text><text class="uni-h5" style="min-width:200rpx;margin-left:-300rpx;">{{"未扫描数量:"+item.lastScanQty}}</text></view></view></view></block></block></view></scroll-view><page-foot class="vue-ref" vue-id="3" data-ref="viewBtm" bind:__l="__l" vue-slots="{{['default']}}"><button type="primary" data-event-opts="{{[['tap',[['onSave',['$event']]]]]}}" bindtap="__e">保存</button></page-foot></view>