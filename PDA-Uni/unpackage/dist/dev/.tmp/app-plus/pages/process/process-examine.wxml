<view><view id="viewHeader"><barcode-input class="vue-ref" vue-id="1" focus value="{{padBinding.deviceName}}" placeholder="扫描设备" data-ref="input_device" data-event-opts="{{[['^onScaned',[['onDeviceScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input><barcode-input class="vue-ref" vue-id="2" value="{{input.empName}}" placeholder="扫描人员" data-ref="input_emp" data-event-opts="{{[['^onScaned',[['onEmpScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input></view><scroll-view style="{{'height:'+(scrollHeight)+';'}}" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view class="uni-list"><block wx:for="{{details}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['uni-list-cell scroll-view-item',item.selectItemClass]}}" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['onSelectedItem',[index]]]]]}}" bindtap="__e"><view class="uni-triplex-row"><view class="uni-triplex-left"><text class="uni-title">{{"设备编码:"+item.deviceNo}}</text><text class="uni-title">{{"设备名称:"+item.deviceName}}</text><text class="uni-title">{{"设备点检周期: "+item.plantCheckTime}}</text><text class="uni-title">{{"建立日期:"+item.createDate}}</text><view><label style="float:left;" class="_span">备注:</label><input style="float:left;display:inline-block;vertical-align:middle;" type="text" placeholder="备注" data-event-opts="{{[['input',[['__set_model',['$0','rem','$event',[]],[[['details','',index]]]]]]]}}" value="{{item.rem}}" bindinput="__e"/></view></view><view class="uni-triplex-right"><text class="uni-h5" style="min-width:200rpx;margin-left:-300rpx;">{{"检点人员:"+item.empNo}}</text><text class="uni-h5" style="min-width:200rpx;margin-left:-300rpx;">{{"点检时间:"+item.checkTime}}</text></view></view></view></block></block></view></scroll-view><page-foot class="vue-ref" vue-id="3" data-ref="viewBtm" bind:__l="__l" vue-slots="{{['default']}}"><button type="primary" data-event-opts="{{[['tap',[['onSave',['$event']]]]]}}" bindtap="__e">保存</button></page-foot></view>