<view><view id="viewHeader"><barcode-input class="vue-ref" vue-id="1" focus="{{true}}" clearScan="{{true}}" placeholder="扫制造单号" data-ref="input_order" data-event-opts="{{[['^onScaned',[['onOrderScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input></view><scroll-view style="{{'height:'+(scrollHeight)+';'}}" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view class="uni-list"><block wx:for="{{details}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['uni-list-cell scroll-view-item',item.selectItemClass]}}" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['onSelectedItem',[index]]]]]}}" bindtap="__e"><view class="uni-triplex-row"><view class="uni-triplex-left"><text class="uni-title">{{"料号:"+(item.partItemNo?item.partItemNo:'未知')}}</text><text class="uni-title">{{'状态:'+(item.workSta==1?'开工':item.workSta==2?'暂停':item.workSta==3?'下线':item.workSta==4?'完工':'未知')+''}}</text><text class="uni-title">{{"上线时间:"+item.sheetDate}}</text></view><view class="uni-triplex-right"><text class="uni-h5" style="min-width:200rpx;margin-left:-300rpx;">{{"设备:"+item.deviceNo}}</text><text class="uni-h5" style="min-width:200rpx;margin-left:-300rpx;">{{"工序:"+item.proName}}</text></view></view></view></block></block></view></scroll-view><page-foot class="vue-ref" vue-id="2" data-ref="viewBtm" bind:__l="__l" vue-slots="{{['default']}}"><button type="primary" data-event-opts="{{[['tap',[['onSave',['$event']]]]]}}" bindtap="__e">下线</button></page-foot><uni-popup-modal vue-id="3" id="popup-modal" show="{{showPrint}}" title="下线日期" data-event-opts="{{[['^hidePopup',[['onShowPrint',[false]]]],['^save',[['onSubmitPrint']]]]}}" bind:hidePopup="__e" bind:save="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="uni-list-cell input-row"><view class="uni-list-cell-left">下线日期:</view><view class="uni-list-cell-db"><view data-event-opts="{{[['tap',[['showDatePicker',['$event']]]]]}}" bindtap="__e">{{m_endDate}}</view></view></view></uni-popup-modal><mpvue-picker class="vue-ref" vue-id="4" mode="dateSelector" pickerValueDefault="{{m_endDate}}" data-ref="mpvuePicker" data-event-opts="{{[['^onConfirm',[['onPickerConfirm']]]]}}" bind:onConfirm="__e" bind:__l="__l"></mpvue-picker></view>