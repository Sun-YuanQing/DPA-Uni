<view><view id="viewHeader"><barcode-input class="vue-ref" vue-id="1" focus="{{true}}" clearScan="{{true}}" placeholder="请扫描工单号" data-ref="input_order" data-event-opts="{{[['^onScaned',[['onOrderScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input><view class="uni-list-cell input-row"><view class="uni-list-cell-left">单号:</view><view class="uni-list-cell-db">{{order_moLot}}</view></view></view><scroll-view style="{{'height:'+(scrollHeight)+';'}}" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view class="uni-list"><block wx:for="{{orderlist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['uni-list-cell scroll-view-item',item.selectItemClass]}}" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['onSelectedItem',[index]]]]]}}" bindtap="__e"><view class="uni-triplex-row"><view class="uni-triplex-left"><text class="uni-title">{{"料号:"+item.partItemNo}}</text><text class="uni-title">{{"料品名称:"+item.partName}}</text><text class="uni-title">{{"数量: "+item.sheetQty}}</text></view><view class="uni-triplex-right" style="min-width:220rpx;"><text class="uni-h5" style="min-width:200rpx;margin-left:-300rpx;">{{"设备:"+item.deviceNo}}</text><text class="uni-h5" style="min-width:200rpx;margin-left:-300rpx;">{{"工序:"+item.proItemNo}}</text></view></view></view></block></block></view></scroll-view></view>