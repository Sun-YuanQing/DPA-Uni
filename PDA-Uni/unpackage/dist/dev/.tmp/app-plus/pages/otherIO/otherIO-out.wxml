<view><view id="viewHeader"><barcode-input class="vue-ref" vue-id="1" focus="true" placeholder="扫描出库单号" data-ref="input_order" data-event-opts="{{[['^onScaned',[['onOrderScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input><barcode-input class="vue-ref" vue-id="2" placeholder="扫描料号" data-ref="input_part" data-event-opts="{{[['^onScaned',[['onPartScaned']]]]}}" bind:onScaned="__e" bind:__l="__l"></barcode-input></view><scroll-view style="{{'height:'+(scrollHeight)+';'}}" scroll-top="{{scrollTop}}" scroll-y="true" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view class="uni-list"><block wx:for="{{order_list}}" wx:for-item="order" wx:for-index="i" wx:key="i"><block><block wx:for="{{order.details}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['uni-list-cell scroll-view-item',item.selectItemClass]}}" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['onSelectedItem',[i]]]]]}}" bindtap="__e"><view class="uni-triplex-row"><view class="uni-triplex-left"><text class="uni-title">{{"单号:"+item.sheetNo}}</text><text class="uni-title">{{"料号:"+item.partItemNo}}</text><text class="uni-title">{{"料品名称:"+item.partName}}</text><text class="uni-title">{{"仓库:"+(item.whName?item.whName:'')+(item.whNo?'('+item.whNo+')':'')}}</text></view><view class="uni-triplex-right"><text class="uni-h5" style="min-width:200rpx;margin-left:-300rpx;">{{"批号:"+(item.inLot?item.inLot:'')}}</text><text class="uni-h5" style="min-width:200rpx;margin-left:-300rpx;">{{"单位:"+item.unitName}}</text><text class="uni-h5" style="min-width:200rpx;margin-left:-300rpx;">{{"数量:"+item.sheetQty}}</text></view></view></view></block></block></block></view></scroll-view></view>